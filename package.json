{
  "name": "@watson/core",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/M1CH3L1US/Watson/tree/Watson"
  },
  "workspaces": {
    "packages": [
      "packages/core",
      "packages/common",
      "packages/testing"
    ]
  },
  "private": true,
  "scripts": {
    "clean": "lerna run clean",
    "build": "lerna run build",
    "build:common": "lerna run build --scope @watson/common",
    "build:core": "lerna run build --scope @watson/core",
    "build:test": "lerna run build --scope @watson/testing",
    "dev:common": "yarn build:common && yarn dev:run",
    "dev:core": "yarn build:core && yarn dev:run",
    "dev:testing": "yarn build:test && yarn dev:run",
    "test": "yarn run build && node packages/testing/dist/main.js",
    "dev:run": "node packages/testing/dist/main.js",
    "dev:bootstrap": "yarn run build && node packages/testing/dist/main.js"
  },
  "keywords": [
    "discord"
  ],
  "authors": [
    "Michael S.",
    "Colin P."
  ],
  "license": "MIT",
  "dependencies": {
    "axios": "^0.21.0",
    "class-transformer": "^0.3.1",
    "class-validator": "^0.12.2",
    "cli-color": "^2.0.0",
    "dayjs": "^1.9.7",
    "discord.js": "^12.5.1",
    "dotenv": "^8.2.0",
    "iterare": "^1.2.1",
    "object-hash": "^2.0.3",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^6.6.3",
    "socket.io": "^3.0.4",
    "tslib": "^2.0.3",
    "uuid": "^8.3.1"
  },
  "devDependencies": {
    "@types/cli-color": "^2.0.0",
    "@types/dotenv": "^8.2.0",
    "@types/node": "^14.14.11",
    "@types/reflect-metadata": "^0.1.0",
    "@types/uuid": "^8.3.0",
    "lerna": "^3.22.1",
    "typescript": "^4.1.2"
  }
}
