{
  "version": 2,
  "projects": {
    "core": {
      "root": "packages/core",
      "sourceRoot": "packages/core/src",
      "projectType": "library",
      "targets": {
        "build": {
          "executor": "@nrwl/node:package",
          "options": {
            "outputPath": "dist/libs/core",
            "tsConfig": "packages/core/tsconfig.json",
            "packageJson": "packages/core/package.json",
            "main": "packages/core/src/index.ts",
            "assets": ["packages/core/*.md"]
          },
          "dependsOn": [
            {
              "target": "build",
              "projects": "dependencies"
            }
          ]
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "packages/core/test/jest.config.js"
          }
        }
      }
    },
    "common": {
      "root": "packages/common",
      "sourceRoot": "packages/common/src",
      "projectType": "library",
      "targets": {
        "build": {
          "executor": "@nrwl/node:package",
          "options": {
            "outputPath": "dist/libs/common",
            "tsConfig": "packages/common/tsconfig.json",
            "packageJson": "packages/common/package.json",
            "main": "packages/common/src/index.ts",
            "assets": ["packages/common/*.md"]
          }
        }
      }
    },
    "platform-discordjs": {
      "root": "packages/platform-discordjs",
      "sourceRoot": "packages/platform-discordjs/src",
      "projectType": "library",
      "targets": {
        "build": {
          "executor": "@nrwl/node:package",
          "options": {
            "outputPath": "dist/libs/platform-discordjs",
            "tsConfig": "packages/platform-discordjs/tsconfig.json",
            "packageJson": "packages/platform-discordjs/package.json",
            "main": "packages/platform-discordjs/src/index.ts",
            "assets": ["packages/platform-discordjs/*.md"]
          }
        }
      }
    },
    "testing": {
      "root": "packages/testing",
      "sourceRoot": "packages/testing/src",
      "projectType": "library",
      "targets": {
        "build": {
          "executor": "@nrwl/node:package",
          "options": {
            "outputPath": "dist/libs/testing",
            "tsConfig": "packages/testing/tsconfig.json",
            "packageJson": "packages/testing/package.json",
            "main": "packages/testing/src/index.ts",
            "assets": ["packages/testing/*.md"]
          }
        }
      }
    },
    "example": {
      "root": "packages/example",
      "sourceRoot": "packages/example/src",
      "projectType": "application",
      "targets": {
        "build": {
          "executor": "@nrwl/node:build",
          "outputs": ["dist/apps/example"],
          "dependsOn": [
            {
              "target": "build",
              "projects": "dependencies"
            }
          ],
          "options": {
            "outputPath": "dist/packages/example",
            "main": "packages/example/src/main.ts",
            "tsConfig": "packages/example/tsconfig.json"
          },
          "configurations": {
            "production": {
              "optimization": true
            }
          }
        },
        "serve": {
          "executor": "@nrwl/node:execute",
          "options": {
            "buildTarget": "example:build"
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "apps/example/jest.config.js"
          }
        }
      }
    }
  },
  "cli": {
    "defaultCollection": "@nrwl/node"
  },
  "generators": {
    "@nrwl/node:library": {
      "js": true
    }
  }
}
