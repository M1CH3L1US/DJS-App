{
  "extends": "./../../tsconfig.json",
  "compilerOptions": {
    "sourceRoot": "src",
    "paths": {
      "@builtin": ["packages/common/src/builtin"],
      "@exceptions": ["packages/common/src/exceptions"],
      "@interfaces/*": ["packages/common/src/interfaces/*"],
      "@decorators/*": ["packages/common/src/decorators/*"],
      "@command/*": ["packages/common/src/command/*"],
      "@constants": ["packages/common/src/constants.ts"]
    }
  },
  "include": ["*.ts", "**/*.ts"]
}
